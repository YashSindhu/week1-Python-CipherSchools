# print function
print("hello \"hello\' ")
 
# escape sequences
print("line A \nline B")

# escape sequence as normal text
print(r"line A \n line B")

#python as calculator
print(2+3)
print(2-3)
print(2*3)
print(4/2) # float sivision
print(4//2) #integer division

# variables
name = "Yash"
print(name)
# Convention for variable baning